<p style="color: green"><%= notice %></p>

<%= render @photo_album %>

<div class="navbar navbar-expand-lg navbar-light bg-light container mt-3">
  <%= link_to "Print this photo album", print_photo_album_path(@photo_album), class: "button btn btn-outline-primary ms-3" %>
  <%= link_to "Edit this photo album", edit_photo_album_path(@photo_album), class: "button btn btn-outline-primary ms-3" %>
  <%= link_to "Back to photo albums", photo_albums_path, class: "button btn btn-outline-primary ms-3" %>
  <%= button_to "Destroy this photo album", @photo_album, method: :delete, class: "button btn btn-outline-danger ms-3" %>
</div>
